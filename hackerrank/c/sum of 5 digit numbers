#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    int n1,n2,n3,n4,n5;
    
    n1 = (n%10);
    n2 = (n%100)/10;
    n3 = (n%1000)/100;
    n4 = (n%10000)/1000;
    n5 = (n%100000)/10000;
    printf("%d",n1+n2+n3+n4+n5);

    //Complete the code to calculate the sum of the five digits on n.
    return 0;
}

